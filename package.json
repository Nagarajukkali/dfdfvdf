{
  "name": "quote-tool",
  "scripts": {
    "docker": "npm install; npm start",
    "ng": "ng",
    "start": "ng serve -c=$npm_package_config_env",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test --code-coverage",
    "e2e": "ng e2e",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss,html}\"",
    "test-ci": "ng test --watch=false --code-coverage",
    "e2e:all": "rm -rf sales-e2e/results && cd sales-e2e && gherkin-testcafe --tags $npm_package_config_e2e_tags; npm run report",
    "e2e:remote": "rm -rf sales-e2e/results && cd sales-e2e && gherkin-testcafe --tags $npm_package_config_e2e_tags remote --qr-code",
    "e2e:live": "rm -rf sales-e2e/results && cd sales-e2e && gherkin-testcafe --tags $npm_package_config_e2e_tags -L",
    "e2e:windows-all": "rd /s /q \"sales-e2e/results\" && cd sales-e2e && node ../../node_modules/gherkin-testcafe/main.js --tags $npm_package_config_e2e_tags",
    "e2e:windows-remote": "rd /s /q \"sales-e2e/results\" && cd sales-e2e && node ../../node_modules/gherkin-testcafe/main.js --tags $npm_package_config_e2e_tags remote --qr-code",
    "report": "node report-generator.js"
  },
  "config": {
    "env": "non-prod",
    "e2e_tags": "@NewCustomerTesting"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "^0.13.7",
    "@angular/animations": "^7.2.4",
    "@angular/common": "^7.2.4",
    "@angular/compiler": "^7.2.4",
    "@angular/core": "^7.2.4",
    "@angular/forms": "^7.2.4",
    "@angular/platform-browser": "^7.2.4",
    "@angular/platform-browser-dynamic": "^7.2.4",
    "@angular/router": "^7.2.4",
    "@ea/ea-commons-models": "^0.1.1",
    "@ea/ea-ui": "^1.13.5",
    "@ea/ng-self-service": "1.2.76",
    "@ngrx/effects": "^7.2.0",
    "@ngrx/router-store": "^7.2.0",
    "@ngrx/store": "^7.2.0",
    "@types/jquery": "^3.3.31",
    "awesome-typescript-loader": "^5.2.1",
    "core-js": "^2.6.4",
    "crypto-js": "^3.1.9-1",
    "cucumber-helper": "0.0.6",
    "dayjs": "^1.8.12",
    "jshashes": "^1.0.8",
    "lodash": "4.17.15",
    "log4js": "4.0.0",
    "ng-inline-svg": "^8.4.1",
    "ng-recaptcha": "^4.2.1",
    "rxjs": "^6.4.0",
    "source-map-loader": "^0.2.4",
    "testcafe-reporter-cucumber-json": "^4.0.0",
    "testcafe-reporter-html-testrail": "^3.1.3",
    "testcafe-reporter-json": "2.2.0",
    "tslib": "^1.9.0",
    "ua-parser-js": "^0.7.20",
    "yamljs": "^0.3.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.1",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "^7.3.1",
    "@angular/compiler-cli": "^7.2.4",
    "@angular/language-service": "^7.2.4",
    "@ngrx/store-devtools": "^7.2.0",
    "@types/jasmine": "^3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.12.24",
    "codelyzer": "~4.5.0",
    "cucumber": "^5.1.0",
    "express": "^4.16.4",
    "gherkin-testcafe": "^2.4.2",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^1.3.1",
    "jasmine-core": "^3.3.0",
    "jasmine-marbles": "^0.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "multiple-cucumber-html-reporter": "^1.13.1",
    "ngrx-store-freeze": "0.2.4",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0",
    "protractor": "~5.4.0",
    "testcafe": "1.8.1",
    "testcafe-browser-provider-android": "0.0.33",
    "testcafe-reporter-json": "2.2.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": ">=3.2.4 <3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern \"apps/**/**/*.{ts,tsx,scss,html}\""
    }
  }
}
